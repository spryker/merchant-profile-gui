{
  "name": "spryker/merchant-profile-gui",
  "type": "library",
  "description": "MerchantProfileGui module",
  "license": "proprietary",
  "require": {
    "php": ">=7.1",
    "spryker/glossary": "^3.0.0",
    "spryker/kernel": "^3.30.0",
    "spryker/locale": "^3.0.0",
    "spryker/merchant-gui-extension": "^1.0.0",
    "spryker/merchant-profile": "^1.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/twig": "^3.0.0",
    "spryker/url": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/gui": "*",
    "spryker/merchant": "*",
    "spryker/testify": "*",
    "spryker/util-text": "*"
  },
  "suggest": {
    "spryker/util-text": "Required to use gui in plugins.",
    "spryker/gui": "Required to use util text in plugins.",
    "spryker/merchant": "Required to use merchant in plugins."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
